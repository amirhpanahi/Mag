@model IEnumerable<Mag.Areas.Admin.Models.Dto.News.NewsListDto>
<div class="row">
    <div class="col-md-12">
        <div class="d-flex align-items-center">
            <span class="rectangle"></span>
            <span class="BesideRectangle">جدیدترین ها</span>
        </div>
    </div>
    @foreach (var item in Model)
    {
        <a href="/News/@item.Slug">
            <div class="col-md-12 mt-4">
                <div class="d-flex align-items-center">
                    <div class="symbol symbol-55px imageForTenCommponent">
                        <img src="~/@item.IndexImageAddress" alt="@item.IndexImageAddressAlt" title="@item.IndexImageAddressTitle">
                    </div>
                    <h6 class="TextForTenCommponent">
                        @item.Title
                    </h6>
                </div>
            </div>
        </a>
    }
</div>